{
    "parserOptions": {
        "ecmaFeatures": {
            "ecmaVersion": 9
        }
    },
    "env": {
        "node": true
    },
    "rules": {
        "eqeqeq": "warn",
        "semi": "warn",
        "curly": "warn",
        "no-empty": "warn",
        "valid-jsdoc": "warn",
        "no-extra-semi": "warn",
        "no-unused-vars": "warn",
        "no-undef": "error"
    }

}